<h2>編集</h2>
model: <%= pp @post_image %>
<%= form_with model: @post_image, local: false do |f| %>
  <label for="inputName">コース名</label>
  <%= f.text_field :couse_name, autofocus: true, id:"inputName", placeholder:"コース名"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :image, placeholder: "画像", accept: "image/*" %>
  <label for="inputCaption">説明</label>
  <%= f.text_field :caption, autofocus: true, id:"inputCaption", placeholder:"説明"%>
  <%= f.submit "変更を保存" %>
<% end %>